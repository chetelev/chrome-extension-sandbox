{
    "manifest_version": 2,
    "name": "iEatBackground",
    "description": "Add a custom background to Google.com",
    "version": "1.0",
    "icons": {
        "16": "./images/16.png",
        "48": "./images/48.png",
        "128": "./images/128.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.google.com/*",
        "storage"
    ]
}